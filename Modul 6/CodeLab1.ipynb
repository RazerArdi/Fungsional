   "source": [
    "from PIL import Image, ImageFilter\n",
    "\n",
    "# Buka gambar\n",
    "img = Image.open('kucing.jpg')\n",
    "\n",
    "# Terapkan filter kontur\n",
    "image_after1 = img.filter(ImageFilter.CONTOUR)\n",
    "\n",
    "# filter BLUR\n",
    "image_after2 = img.filter(ImageFilter.BLUR)\n",
    "\n",
    "# Tampilkan hasil\n",
    "image_after1.show()\n",
    "image_after2.show()"
   ]
